{
  "AWS": {
    "Region": "us-east-1",
    "Profile": "local-development"
  },
 "DynamoDB": {
    "TableName": "Accounts",
    "Region": "us-east-1",
    "LocalMode": true,
    "LocalServiceUrl": "http://localhost:8000",
    "MaxRetries": 3,
    "RetryMode": "Standard",
    "TTLEnabled": true,
    "TTLAttributeName": "ExpirationTime"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
